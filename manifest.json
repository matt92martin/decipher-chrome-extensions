{
    "manifest_version": 2,

    "name": "FocusVision Keyboard Shortcuts for Decipher",
    "description": "This extension provides keyboard shortcuts for navigating around surveys",
    "version": "2.2.4",
    "author": "matt92martin",
    "icons": {
        "128": "src/Deciphericon.png" 
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": [
            "src/css/styles.css"
        ],
        "js": [
            "src/js/jquery-3.1.0.min.js",
            "src/js/AnswerForMe.js",
            "src/js/Shortcuts.js",
            "src/js/QuotaBuddy.js",
            "src/js/keybind.js",
            "src/js/main.js"

        ],
        "run_at": "document_end"
    }], 
    "background": {
        "scripts": [
            "src/js/background.js"
        ]
    },
    "browser_action": {
        "default_icon": "src/Deciphericon.png",
        "default_title": "EZ FV",
        "default_popup": "src/popup/popup.html"
    },

    "permissions": [
        "storage",
        "history",
        "tabs",
        "bookmarks",
        "unlimitedStorage"
    ],
    "options_ui": {
        "page": "src/options.html"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+L",
                "windows": "Ctrl+Shift+L",
                "mac": "Command+Shift+L"
            }
        }
    }
}
