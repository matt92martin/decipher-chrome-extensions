{
    "manifest_version": 2,

    "name": "FocusVision Keyboard Shortcuts for the Decipher platform",
    "description": "This extension provides keyboard shortcuts for navigating around surveys",
    "version": "2016.11.1",



    "icons": { 
        "128": "src/Deciphericon.png" 
    },
    "web_accessible_resources": [
        "src/quotaBuddy.css"
    ],

    "content_scripts": [
        {
            "matches": [
                "https://*.decipherinc.com/*",
                "http://*.decipherinc.com/*"
            ],
            "css": [
                "src/quotaBuddy.css"
            ],
            "js": [
                "src/jquery-3.1.0.min.js",
                "src/Shortcuts.js",
                "src/AnswerForMe.js"
            ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_title": "TimeLines",
        "default_icon": "src/Deciphericon.png",
        "default_popup": "src/popup.html"
    },


    "permissions": ["storage","history"]
}
