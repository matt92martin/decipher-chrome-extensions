{
    "manifest_version": 2,

    "name": "FocusVision Keyboard Shortcuts for the Decipher platform",
    "description": "This extension provides keyboard shortcuts for navigating around surveys",
    "version": "2016.11.1",
    "options_ui": {
      "page": "src/options.html",
      "chrome_style": true
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

    "icons": {
        "128": "src/Deciphericon.png" 
    },
    "web_accessible_resources": [
        "src/css/quotaBuddy.css"
    ],

    "content_scripts": [
        {
            "matches": [
                "https://*.decipherinc.com/*",
                "http://*.decipherinc.com/*"
            ],
            "css": [
                "src/css/quotaBuddy.css"
            ],
            "js": [
                "src/js/jquery-3.1.0.min.js",
                "src/js/Shortcuts.js",
                "src/js/AnswerForMe.js"
            ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_title": "TimeLines",
        "default_icon": "src/Deciphericon.png",
        "default_popup": "src/popup.html"
    },


    "permissions": ["storage","history","tabs"]
}
