{
    "manifest_version": 2,

    "name": "FocusVision Keyboard Shortcuts for Decipher",
    "description": "This extension provides keyboard shortcuts for navigating around surveys",
    "version": "2.2.0",
    "author": "matt92martin",
    "icons": {
        "128": "src/Deciphericon.png" 
    },
    "applications": {
        "gecko": {
            "id": "{2233c1e5-d293-4035-bf49-b03293ff63d5}"
        }
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": [
            "src/css/styles.css"
        ],
        "js": [
            "src/js/jquery-3.1.0.min.js",
            "src/js/AnswerForMe.js",
            "src/js/Shortcuts.js",
            "src/js/QuotaBuddy.js",
            "src/js/keybind.js",
            "src/js/main.js"

        ],
        "run_at": "document_end"
    }], 
    "background": {
        "scripts": [
            "src/js/background.js"
        ]
    },
    "browser_action": {
        "default_icon": "src/Deciphericon.png",
        "default_title": "EZ FV",
        "default_popup": "src/popup/popup.html"
    },

    "permissions": [
        "storage",
        "history",
        "tabs",
        "bookmarks",
        "unlimitedStorage"
    ],
    "options_ui": {
        "page": "src/options.html"
    }
}
